{
  "name": "backend-meang",
  "version": "0.2.2",
  "description": "Backend Project API GraphQL Online Store - MEAN + G",
  "main": "build/server.js",
  "scripts": {
    "start": "node build/server.js",
    "build": "tsc -p .",
    "start:dev": "cross-env NODE_ENV=development nodemon \"src/server.ts\" --exec \"ts-node\" \"src/server.ts\" -e ts,graphql,json",
    "start:staging": "cross-env NODE_ENV=staging nodemon \"src/server.ts\" --exec \"ts-node\" \"src/server.ts\" -e ts,graphql,json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhoncharry/backend-meang-online-store.git"
  },
  "keywords": [
    "graphql",
    "meang",
    "api-graphql",
    "stipe",
    "cloudinary",
    "netlify",
    "zeitnow"
  ],
  "author": "Jhon Charry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jhoncharry/backend-meang-online-store/issues"
  },
  "homepage": "https://github.com/jhoncharry/backend-meang-online-store#readme",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@graphql-tools/load-files": "^6.4.0",
    "@graphql-tools/merge": "^8.1.2",
    "@types/jsonwebtoken": "^8.5.5",
    "apollo-server-express": "^3.3.0",
    "bcrypt": "^5.0.1",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.3",
    "graphql-import-node": "^0.0.4",
    "graphql-playground-middleware-express": "^1.7.22",
    "graphql-tools": "^8.2.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.5",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.12",
    "randexp": "^0.5.3",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
